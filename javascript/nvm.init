loadnvm() {
  if [ -z $NVM_LOADED ]; then
    echo "[nvm]: not loaded, sourcing nvm.sh"

    export NVM_LOADED=1
    export NVM_DIR="$HOME/.nvm"
    [ -s "$NVM_DIR/nvm.sh" ] && \. "$NVM_DIR/nvm.sh"
  fi
}

# nvm slows down shell load times
# only source it when required
nvm() {
  loadnvm
  nvm $@
}


